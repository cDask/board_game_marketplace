<table>
    <tr>
        <th>Board Game Name</th>
        <th>Condition</th>
        <th>List Type</th>
        <th>Price</th>
        <th>Board Game to Trade For</th>
    </tr>
    <% listings.each do |listing| %>
    <tr>
        <td><%= listing.board_game_name %></td>
        <td><%= listing.condition %></td>
        <td><%= listing.list_type %></td>
        <td><%= listing.price %></td>
        <td><%= listing.board_game_trade %></td>
    </tr>
    <% end %>
</table>