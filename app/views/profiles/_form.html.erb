<%= form_with model: profile, local: true do |f| %>
<%= f.label :username %>
<%= f.text_field :username %>
<%= f.label :profile_pic %> 
<%= f.file_field :profile_pic, accept: 'image/png,image/gif,image/jpeg,image/jpg' %>
<%= f.label :rating %>
<%= f.number_field :rating, in: 100..500, step: 100 %>
<%#= f.label :location %>
<%#= f.text_field :location %>
<%= f.submit%>
<% end %>